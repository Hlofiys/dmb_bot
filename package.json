{
  "name": "belarus-army-bot",
  "version": "1.0.0",
  "description": "Telegram bot to track military service in Belarus",
  "type": "module",
  "module": "src/index.ts",
  "main": "dist/index.js",
  "scripts": {
    "start": "bun run dist/index.js",
    "build": "bun build --target=bun src/index.js --outfile=dist/index.js",
    "dev": "tsx src/index.ts",
    "lint": "eslint src --ext .ts,.js",
    "lint:fix": "eslint src --ext .ts,.js --fix",
    "format": "prettier --write \"src/**/*.{ts,js,json}\"",
    "format:check": "prettier --check \"src/**/*.{ts,js,json}\"",
    "check": "bun run lint && bun run format:check"
  },
  "keywords": ["telegram", "bot", "grammyjs", "belarus", "army"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^17.2.3",
    "grammy": "^1.38.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/node": "^24.9.1",
    "eslint": "^9.38.0",
    "globals": "^16.4.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
